{
  "name": "vite-react",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev:vite": "vite",
    "build:vite": "tsc && vite build",
    "dev:server": "ts-node-dev -P tsconfig.server.json server/main.ts",
    "build:server": "webpack --config ./webpack.config.cjs",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:vite\" --kill-others",
    "start": "npm run build:server && && npm run build:vite && node dist/resources/main.js",
    "build": "npm run build:vite && npm run build:server",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-osc": "^8.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "@types/node-osc": "^6.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}
